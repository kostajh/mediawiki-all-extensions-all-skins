{
	"name": "Disambiguator",
	"version": "1.3",
	"author": [
		"Ryan Kaldari"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Disambiguator",
	"descriptionmsg": "disambig-desc",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.29.0"
	},
	"type": "other",
	"ExtensionMessagesFiles": {
		"DisambiguatorAlias": "Disambiguator.i18n.alias.php",
		"DisambiguatorMagic": "Disambiguator.i18n.magic.php"
	},
	"Hooks": {
		"GetDoubleUnderscoreIDs": "DisambiguatorHooks::onGetDoubleUnderscoreIDs",
		"wgQueryPages": "DisambiguatorHooks::onwgQueryPages",
		"AncientPagesQuery": "DisambiguatorHooks::onAncientPagesQuery",
		"LonelyPagesQuery": "DisambiguatorHooks::onLonelyPagesQuery",
		"ShortPagesQuery": "DisambiguatorHooks::onShortPagesQuery",
		"RandomPageQuery": "DisambiguatorHooks::onRandomPageQuery",
		"GetLinkColours": "DisambiguatorHooks::onGetLinkColours"
	},
	"MessagesDirs": {
		"Disambiguator": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.disambiguator.visualEditor": {
			"scripts": [
				"visualEditorIntegration.js"
			],
			"messages": [
				"visualeditor-dialog-meta-settings-disambiguation-label"
			],
			"dependencies": [
				"ext.visualEditor.mwmeta",
				"ext.visualEditor.mediawiki"
			],
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Disambiguator/modules"
	},
	"config": {
		"DisambiguatorIndicateLinks": {
			"value": true
		}
	},
	"SpecialPages": {
		"DisambiguationPages": "SpecialDisambiguationPages",
		"DisambiguationPageLinks": "SpecialDisambiguationPageLinks"
	},
	"AutoloadClasses": {
		"DisambiguatorHooks": "includes/DisambiguatorHooks.php",
		"SpecialDisambiguationPages": "includes/specials/SpecialDisambiguationPages.php",
		"SpecialDisambiguationPageLinks": "includes/specials/SpecialDisambiguationPageLinks.php"
	},
	"attributes": {
		"VisualEditor": {
			"PluginModules": [
				"ext.disambiguator.visualEditor"
			]
		}
	},
	"manifest_version": 2
}
